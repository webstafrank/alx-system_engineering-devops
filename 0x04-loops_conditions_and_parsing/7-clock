#!/usr/bin/env bash

# Displaying the time from 0 to 12 hours and 1 to 59 minutes

# Initialize variables for hours and minutes
hour=0

# Using a while loop to iterate over hours from 0 to 12
while [ $hour -le 12 ]; do
    echo "Hour: $hour"

    # Display minutes from 1 to 59 for each hour
    minute=1
    while [ $minute -le 59 ]; do
        echo $minute
        (( minute++ ))
    done

    (( hour++ ))
done
